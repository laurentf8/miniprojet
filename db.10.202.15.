;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     21.15.202.10.in-addr.arpa root.localhost. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      laurent-ferrin.local
10.202.15.21   IN      PTR     localhost.
21      IN      PTR     laurent-ferrin.local


